<p id="notice"><%= notice %></p>

<h1>Assign Plugin Configs</h1>

<table>
  <thead>
    <tr>
      <th>Assignment</th>
      <th>Plugin</th>
      <th>Subtype</th>
      <th>Name</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @assign_plugin_configs.each do |assign_plugin_config| %>
      <tr>
        <td><%= assign_plugin_config.assignment %></td>
        <td><%= assign_plugin_config.plugin %></td>
        <td><%= assign_plugin_config.subtype %></td>
        <td><%= assign_plugin_config.name %></td>
        <td><%= assign_plugin_config.value %></td>
        <td><%= link_to 'Show', assign_plugin_config %></td>
        <td><%= link_to 'Edit', edit_assign_plugin_config_path(assign_plugin_config) %></td>
        <td><%= link_to 'Destroy', assign_plugin_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Assign Plugin Config', new_assign_plugin_config_path %>
