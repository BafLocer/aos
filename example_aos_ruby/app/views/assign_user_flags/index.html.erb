<p id="notice"><%= notice %></p>

<h1>Assign User Flags</h1>

<table>
  <thead>
    <tr>
      <th>Userid</th>
      <th>Assignment</th>
      <th>Locked</th>
      <th>Mailed</th>
      <th>Extensionduedate</th>
      <th>Workflowstate</th>
      <th>Allocatedmarker</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @assign_user_flags.each do |assign_user_flag| %>
      <tr>
        <td><%= assign_user_flag.userid %></td>
        <td><%= assign_user_flag.assignment %></td>
        <td><%= assign_user_flag.locked %></td>
        <td><%= assign_user_flag.mailed %></td>
        <td><%= assign_user_flag.extensionduedate %></td>
        <td><%= assign_user_flag.workflowstate %></td>
        <td><%= assign_user_flag.allocatedmarker %></td>
        <td><%= link_to 'Show', assign_user_flag %></td>
        <td><%= link_to 'Edit', edit_assign_user_flag_path(assign_user_flag) %></td>
        <td><%= link_to 'Destroy', assign_user_flag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Assign User Flag', new_assign_user_flag_path %>
